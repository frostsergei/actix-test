     "version": "0.2.0",
     "configurations": [
          {
               "name": "(gdb) Запустить",
               "type": "cppdbg",
               "request": "launch",
               "program": "${workspaceFolder}/target/debug/rust-actix-test.exe",
               "args": [],
               "stopAtEntry": false,
               "cwd": "${workspaceFolder}",
               "environment": [],
               "externalConsole": false,
               "MIMode": "gdb",
               "miDebuggerPath": "/path/to/gdb",
               "setupCommands": [
                   {
                       "description": "Включить автоматическое форматирование для gdb",
                       "text": "-enable-pretty-printing",
                       "ignoreFailures": true
                   }
               ]
          },
          {
               "name": "(Windows) Запустить",
               "type": "cppvsdbg",
               "request": "launch",
               "program": "${workspaceFolder}/target/debug/rust-actix-test.exe",
               "args": [],
               "stopAtEntry": false,
               "cwd": "${workspaceFolder}",
               "environment": [],
               "externalConsole": false
          },
          {
               "type": "lldb",
               "request": "launch",
               "name": "Debug",
               "program": "${workspaceFolder}/target/debug/rust-actix-test.exe",
               "args": [],
               "cwd": "${workspaceFolder}"
          }
     ]
}